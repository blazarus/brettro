{
    "name": "brettro",
    "version": "0.0.1",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint --type-check",
        "e2e": "ng e2e",
        "server": "nodemon --watch server/**/*.ts --exec ts-node ./server/index.ts",
        "codegen:introspect": "apollo-codegen introspect-schema server/schema.graphql --output src/generated/schema.json",
        "codegen:generate": "apollo-codegen generate './src/**/*.ts' --schema src/generated/schema.json --target typescript --output src/generated/query-types.ts",
        "codegen": "npm run codegen:introspect && npm run codegen:generate"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.2.3",
        "@angular/cdk": "^2.0.0-beta.8",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/material": "^2.0.0-beta.6",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "apollo-angular": "^0.13.0",
        "apollo-client": "^1.4.2",
        "apollo-codegen": "^0.16.1",
        "core-js": "^2.4.1",
        "express": "^4.15.3",
        "graphql": "^0.10.1",
        "graphql-server-express": "^1.0.0",
        "graphql-subscriptions": "^0.4.3",
        "graphql-tag": "^2.4.0",
        "graphql-tools": "^1.0.0",
        "immutability-helper": "^2.3.0",
        "lodash": "^4.17.4",
        "rxjs": "^5.1.0",
        "subscriptions-transport-ws": "^0.8.0",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "^1.1.2",
        "@angular/compiler-cli": "^4.0.0",
        "@types/express": "^4.0.36",
        "@types/graphql": "^0.10.0",
        "@types/jasmine": "^2.5.52",
        "@types/lodash": "^4.14.69",
        "@types/node": "^8.0.9",
        "codelyzer": "^3.0.1",
        "jasmine-core": "^2.6.4",
        "jasmine-spec-reporter": "^4.1.0",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.3.0",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "nodemon": "^1.11.0",
        "protractor": "~5.1.0",
        "ts-node": "^3.0.6",
        "tslint": "^5.4.3",
        "typescript": "2.3.4"
    }
}
